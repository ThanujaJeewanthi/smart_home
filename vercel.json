{
  "version": 2,
  "builds": [
    {
      "src": "webApp/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "webApp/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/webApp/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/webApp/$1"
    }
  ]
}